<h1>Listing trades</h1>

<table>
  <tr>
    <th>Students</th>
    <th>Sections</th>
    <th>Course</th>
    <th>Project</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @trades.each do |trade| %>
  <tr>
    <td><%= trade.students %></td>
    <td><%= trade.sections %></td>
    <td><%= trade.Course %></td>
    <td><%= trade.Project %></td>
    <td><%= trade.status %></td>
    <td><%= link_to 'Show', trade %></td>
    <td><%= link_to 'Edit', edit_trade_path(trade) %></td>
    <td><%= link_to 'Destroy', trade, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Trade', new_trade_path %>
